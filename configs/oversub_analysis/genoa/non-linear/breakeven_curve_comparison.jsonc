{
  "name": "breakeven_curve_comparison",
  "description": "Aggregate breakeven vCPU demand multipliers across utilization levels, comparing with and without resource scaling",
  "analysis": {
    "type": "breakeven_curve",
    "series": [
      {
        "label": "Without Resource Scaling",
        "configs": [
          "configs/oversub_analysis/genoa/non-linear/util_10_pct.jsonc",
          "configs/oversub_analysis/genoa/non-linear/util_20_pct.jsonc",
          "configs/oversub_analysis/genoa/non-linear/util_30_pct.jsonc"
        ]
      },
      {
        "label": "With Resource Scaling",
        "configs": [
          "configs/oversub_analysis/genoa/non-linear/util_10_pct_resource_scaling.jsonc",
          "configs/oversub_analysis/genoa/non-linear/util_20_pct_resource_scaling.jsonc",
          "configs/oversub_analysis/genoa/non-linear/util_30_pct_resource_scaling.jsonc"
        ]
      }
    ],
    "x_parameter": "workload.avg_util",
    "x_display_multiplier": 100,
    "breakeven_metric": "carbon",
    "x_label": "Average Utilization (%)",
    "y_label": "Breakeven vCPU Demand Multiplier"
  },
  "output_dir": "./results/oversub_analysis/genoa/non-linear/breakeven_curve_comparison"
}
