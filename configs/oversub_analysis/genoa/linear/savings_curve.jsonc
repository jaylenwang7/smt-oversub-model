{
  "name": "savings_at_discount_points",
  "description": "Savings of nosmt_oversub vs smt_oversub at low/geomean/high vCPU discount points across utilization levels (with resource scaling)",
  "analysis": {
    "type": "savings_curve",
    "configs": [
      "configs/oversub_analysis/genoa/linear/util_10_pct_linear_resource_scaling.jsonc",
      "configs/oversub_analysis/genoa/linear/util_20_pct_linear_resource_scaling.jsonc",
      "configs/oversub_analysis/genoa/linear/util_30_pct_linear_resource_scaling.jsonc",
      "configs/oversub_analysis/genoa/linear/util_40_pct_linear_resource_scaling.jsonc",
      "configs/oversub_analysis/genoa/linear/util_50_pct_linear_resource_scaling.jsonc"
    ],
    "x_parameter": "workload.avg_util",
    "x_display_multiplier": 100,
    "x_label": "Average Utilization (%)",
    "y_label": "Savings vs SMT Baseline (%)",
    "marker_values": [0.65, 0.75, 0.85],
    "marker_labels": ["low", "geomean", "high"],
    "metrics": ["carbon", "tco"]
  },
  "output_dir": "./results/oversub_analysis/genoa/linear/savings_curve"
}
