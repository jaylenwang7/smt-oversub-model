{
  "name": "savings_curve_old_vs_new_oversub",
  "description": "Geomean savings comparison: old vs new oversub ratios across utilization levels (10-30%)",
  "analysis": {
    "type": "savings_curve",
    "config_sets": [
      {
        "label": "Old oversub",
        "configs": [
          "configs/oversub_analysis/genoa/linear/util_10_pct_linear_resource_scaling.jsonc",
          "configs/oversub_analysis/genoa/linear/util_20_pct_linear_resource_scaling.jsonc",
          "configs/oversub_analysis/genoa/linear/util_30_pct_linear_resource_scaling.jsonc"
        ]
      },
      {
        "label": "New oversub",
        "configs": [
          "configs/oversub_analysis/genoa/linear/util_10_pct_linear_resource_scaling_new_oversub.jsonc",
          "configs/oversub_analysis/genoa/linear/util_20_pct_linear_resource_scaling.jsonc",
          "configs/oversub_analysis/genoa/linear/util_30_pct_linear_resource_scaling_new_oversub.jsonc"
        ]
      }
    ],
    "x_parameter": "workload.avg_util",
    "x_display_multiplier": 100,
    "x_label": "Average Utilization (%)",
    "y_label": "Savings vs SMT Baseline (%)",
    "marker_values": [0.75],
    "marker_labels": ["geomean"],
    "metrics": ["carbon", "tco"],
    "legend_title": "Oversub ratios",
    "plot": {
      "figsize": [8, 5]
    }
  },
  "output_dir": "./results/oversub_analysis/genoa/linear/savings_curve_oversub_comparison"
}
